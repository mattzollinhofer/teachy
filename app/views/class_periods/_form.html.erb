<%= semantic_form_for class_period do |form| %>
  <%= form.inputs do %>
    <%= form.input :course, input_html: {class: 'chosen-select'} %>
    <%= form.input :teachers, input_html: {class: 'chosen-select'} %>
    <%= form.input :period %>
  <% end %>
  <%= form.actions do %>
    <%= form.action :submit %>
  <% end %>
<% end %>
